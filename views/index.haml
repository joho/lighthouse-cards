.wrapper
  - @tickets.each do |t|
    .ticket
      .ticket_title
        = t.title
      .meta
        .value
          .box
          %span{ :class => 'metatxt' }
            effort
        .effort
          .box
          %span{ :class => 'metatxt' }
            value
      .ticket_number
        = "\##{t.id}"

